vowel or consonant
#include <stdio.h>

int main()
{
    char a;
    scanf("%c",&a);
    if(a=='a'||a=='e'||a=='i'||a=='o'||a=='u'||a=='A'||a=='E'||a=='I'||a=='O'||a=='U')
    {
        printf("it is vowel");
        
    }
    else
    {
        printf(" it is  a consonant");
    }

    return 0;
}
