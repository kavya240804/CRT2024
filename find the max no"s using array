find the maximum of numbers  by using array concept
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a[100],i,max,n;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
        max=a[0];
    for(i=1;i<n;i++)
    {
      if(max<a[i])
           {
               max=a[i];
           }
        
    }
      printf("%d\n",max);
    return 0;
}
